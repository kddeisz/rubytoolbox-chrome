{
  "name": "Ruby Toolbox",
  "version": "1.0",
  "description": "View Ruby Toolbox metadata on rubygems.org",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://rubygems.org/gems/*"],
      "css": ["contentScript.css"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  }
}
